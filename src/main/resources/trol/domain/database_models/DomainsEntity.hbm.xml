<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="trol.domain.database_models.DomainsEntity" table="domains" schema="estacho1">
        <id name="idDomain" column="id_domain">
            <generator class="native"/>
        </id>
        <property name="domainString" column="domain_string"/>
        <many-to-one name="idDomainsList" column="id_domains_list"/>
    </class>
</hibernate-mapping>